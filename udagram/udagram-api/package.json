{
  "name": "udagram-api",
  "version": "2.0.0",
  "description": "",
  "engines": {
    "node": "16.0.0"
  },
  "main": "src/server.ts",
  "scripts": {
    "start": "node server.js",
    "tsc": "npx tsc",
    "dev": "npx ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "prod": "npx tsc && node ./www/server.js",
    "clean": "rm -rf www/ || true",
    "setenv": "eb setenv PORT=$PORT POSGRES_DB=$POSGRES_DB POSTGRES_HOST=$POSTGRES_HOST POSTGRES_PASSWORD=$POSTGRES_PASSWORD POSTGRES_USERNAME=$POSTGRES_USERNAME DB_PORT=$DB_PORT AWS_DEFAULT_REGION=$AWS_DEFAULT_REGION AWS_PROFILE=$AWS_PROFILE AWS_BUCKET=$AWS_BUCKET JWT_SECRET=$JWT_SECRET",
    "deploy": "chmod +x bin/ebDeploy.sh && bin/ebDeploy.sh",
    "build": "npm run clean && npm run tsc && cp .npmrc www/.npmrc && cp package.json www/package.json && cd www && zip -r Archive.zip . && cd ..",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Ibrahim Ahmed",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "8.3.2",
    "aws-sdk": "2.429.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "email-validator": "2.0.4",
    "express": "4.16.4",
    "jsonwebtoken": "8.5.1",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "sequelize": "^6.21.6",
    "sequelize-typescript": "^2.1.3"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.26",
    "@types/cors": "2.8.6",
    "@types/express": "4.16.1",
    "@types/node": "^18.7.18",
    "@types/sequelize": "^4.28.14",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "chai": "4.2.0",
    "chai-http": "4.2.1",
    "eslint": "6.8.0",
    "eslint-config-google": "0.14.0",
    "mocha": "6.1.4",
    "ts-node-dev": "1.0.0-pre.32",
    "typescript": "^4.2.3"
  }
